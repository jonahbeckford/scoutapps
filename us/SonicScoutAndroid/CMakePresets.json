      "name": "ci-gradle-ninja-configure",
      "displayName": "Gradle with Ninja",
      "description": "Use after Gradle creates the CMake cache with the Ninja build tool. Example: ./dk dksdk.ninja.link ; ./dk dksdk.gradle.run ARGS cmakeConfigure -P cmakeGenerator=Ninja '-PcmakeMakeProgram=\".ci/ninja/bin/ninja\"'",
      "binaryDir": "build_PRESET",
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_MAKE_PROGRAM": "${sourceDir}/.ci/ninja/bin/ninja"
      }
    },
    {
      "name": "ci-agnostic-configure",
      "description": "ABI-agnostic CI configuration for build and test presets",
      "binaryDir": "build_PRESET",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "DKSDK_GENERATOR": "DKCODER",
        "DKSDK_GOLDEN_SOURCE_CODE": "ON",
        "FETCHCONTENT_SOURCE_DIR_DKSDK-ACCESS": {
          "type": "FILEPATH",
          "value": "${sourceDir}/fetch/dksdk-access"
        },
        "FETCHCONTENT_SOURCE_DIR_DKSDK-CMAKE": {
          "type": "FILEPATH",
          "value": "${sourceDir}/fetch/dksdk-cmake"
        }
      }
    },
